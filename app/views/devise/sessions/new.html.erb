
<div id="main">	

  <div id="site_content">
<h2>Log in</h2>
<hr>
<section id="login">
<div id="login">
 <div class="form-wrap">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div id="form-group">
  <div id="titles"><%= f.label :"Email Address" %></div>
 <div id="loginfields"> <%= f.email_field :email, autofocus: true %></div>
 
 </div>
<br />
  <div id="form-group">
  <div id="titles"><%= f.label :password %></div>
   <div id="loginfields"> <%= f.password_field :password, autocomplete: "off" %></div></div>
<br />
  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
  <% end -%>

  <div id="signinbutton"><%= f.submit "Log in" %></div>
<% end %>
<%= render "devise/shared/links" %>
</div> 
</div> 
</section>
</div>
</div>
